{
  "//": "this is the second line of the comment",
  "name": "@1ak31sha/ui-template",
  "version": "1.3.3",
  "main": "./lib/index.js",
  "scarfSettings.enabled": false,
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "prestart": "yarn run gen:docs",
    "start": "npm-run-all --parallel start:docs gen:docs-watch",
    "start:docs": "node scripts/start.js",
    "gen:docs": "node scripts/generateComponentData.js",
    "gen:docs-watch": "yarn run gen:docs --watch",
    "build:docs": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm-run-all --parallel build:commonjs build:css build:copy-files",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "predeploy:docs": "npm run build:docs",
    "deploy:docs": "gh-pages -d build",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js",
    "prepublish": "npm run build:lib"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "emotion": "^10.0.27",
    "highlight.js": "^9.10.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-table": "^7.0.4",
    "styled-components": "^1.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "autoprefixer": "6.7.2",
    "babel-eslint": "7.2.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^2.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "1.1.3",
    "chokidar": "^1.6.1",
    "connect-history-api-fallback": "1.3.0",
    "cpx": "^1.5.0",
    "cross-env": "^4.0.0",
    "cross-spawn": "4.0.2",
    "css-loader": "^3.4.2",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "gh-pages": "^0.12.0",
    "highlight.js": "^9.10.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "http-proxy-middleware": "0.17.3",
    "inquirer": "^7.0.4",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "postcss-loader": "^3.0.0",
    "promise": "7.1.1",
    "react-addons-test-utils": "^15.5.1",
    "react-dev-utils": "^10.1.0",
    "react-docgen": "^2.14.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "style-loader": "^1.1.3",
    "url-loader": "0.5.7",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "whatwg-fetch": "2.0.2"
  },
  "author": {
    "name": "Lakeisha Pogrzeba",
    "email": "lakeisha.pogrzeba@gmail",
    "url": "https://www.lakeisha.ca"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "components",
    "react",
    "react-component"
  ],
  "homepage": "http://1ak31sha.github.io/lakeui/",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [],
    "plugins": []
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
